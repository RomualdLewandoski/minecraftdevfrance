{% extends 'base.html.twig' %}

{% block title %}Hello ProfileCookieController!{% endblock %}

{% block body %}
    <div class="container mt-2 pl-4">
        <section class="content-header pl-1">
            <ol class="breadcrumb">
                <li>
                    <a href="#">
                        <i class="fa fa-user"></i> Espace membre
                    </a>
                </li>
                <li>EasterEgg Cookie</li>
            </ol>
        </section>
        <div class="mt-5">
            <div class="cookie"
                 style="background-image: url('{{ asset('assets/images/cookieLayout.png') }}'); background-repeat: no-repeat; background-size: cover; min-height: 512px; padding-top: 20px; position: relative">
                <div class="text-center h3"><i class="fad fa-cookie"></i> <span id="cookieNumber"> 0 </span></div>
                <div class="row mt-5">
                    <div class="col-md-6 text-center mb-4">
                        <p class="h4 mb-3">Actions:</p>
                        <button class="btn btn-success" id="craftCookie"><i class="fad fa-cookie-bite"></i> Crafter des
                            cookies : <span id="craftCookieNumber">1</span></button>
                        <br>
                    </div>

                    <div class="col-md-6 text-center mb-4">
                        <p class="h4">Upgrades:</p>
                        <button class="btn btn-warning mb-3" id="upgradeField"><i class="fad fa-tools"></i> Am√©liorer
                            les
                            champs (Lvl : <span id="fieldNumber">1</span> ) Prix: <span id="fieldUpgradePrice"></span>
                            - <span id="filedCookie"></span><i class="fad fa-cookie-bite"></i>
                        </button>
                        <button class="btn btn-danger mb-3" id="upgradeVillager"><i class="fad fa-bed"></i> Embaucher
                            des
                            villageois (Lvl : <span id="villagerNumber">0</span>) Prix: <span
                                    id="villagerUpgradePrice"></span>
                            - <span id="villagerCookie"></span><i class="fad fa-cookie-bite"></i>
                            <div class="progress">
                                <div class="progress-bar progress-bar-striped bg-warning" role="progressbar"
                                     aria-valuenow="0" aria-valuemin="0"
                                     aria-valuemax="100"></div>
                            </div>
                        </button>

                        <button class="btn btn-success mb-3" id="upgradeGolem"><i class="fad fa-tools"></i> Spawner un
                            golem
                            (Lvl : <span id="golemNumber">1</span> ) Prix: <span id="golemUpgradePrice"></span>
                        </button>

                        <br>
                    </div>
                </div>
            </div>

        </div>
